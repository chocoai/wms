                    <BillUIWidget Key="submit" Caption="提交"
                                  EnabledWhen="True"
                                  Icon="glyphicon glyphicon-floppy-saved" WidgetType="BillUIButton">
                        <Triggers>
                            <Trigger Type="javascript/angular" Runat="client">
                                <![CDATA[
                                        var name=getHeadValue("jixing");
                                        var mingcheng=getHeadValue("shangpinmingcheng");
                                        if(name==0){
                                           alert("请填写商品剂型信息");
                                            return;
                                        }
                                        if(mingcheng==''){
                                           alert("请填写商品名称信息");
                                            return;
                                        }
                                        if(getHeadValue("xiaoqidanwei")==0){
                                           alrt("请填写效期单位信息");
                                            return;
                                        }
                                        if(getHeadValue("jixing")==0){
                                           alrt("请填写剂型信息");
                                            return;
                                        }
                                        if(getHeadValue("danwei")==0){
                                           alrt("请填写单位信息");
                                            return;
                                        }
                                          var v=getHeadValue("shangpinleibie");
                                             if(v>=1&&v<=7){
                                                if(getHeadValue("chufangfenlei")==0){
                                                alert("请填写处方分类信息");
                                                return;
                                             }
                                        }
                                        if(getStatus()==30){
                                        	alert("单据已经提交修改，不能编辑");
                                        }else{
                                            var zhujimaFunc=@submit.zhujimaFunc(mingcheng);

                                            zhujimaFunc.then(function(data) {
                                                 var zhu=eval("("+data+")");
                                                 if(zhu.result[0].zhujima){
                                                        var zhujima=zhu.result[0].zhujima;
                                                        setHeadValue("zhujima",zhujima);
                                                     }
                                                      @$this.save('type',{
                                                     before:{
                                                        setStatus:[30]
                                                     }
                                                     });
                                                    } ,function(e){
                                                });
                                         }
	                                  ]]>
                            </Trigger>
                        </Triggers>
