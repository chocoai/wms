        <!--   
        
        
             单据编号	varchar	规则：YBM+年月日时分秒+3位随机码, 例如：YBM20160426112240764
        -->
        <Field FieldKey="danjubianhao" FieldName="单据编号" FieldType="Varchar" Bound="50" DefaultValue="='XT'+o.orderNo2()" Comment="单据编号"/>
