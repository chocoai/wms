<?xml version="1.0" encoding="utf-8"?>
<DataSet DataSource="" Version="1.0.0">
	<DataTable Key="caigoudingdan" BillType="Bill" Caption="采购订单"
		DefaultFilter="" TableName="Table::xyy_wms_bill_caigoudingdan"
		DataSource="" Head="true" >
		
		<!-- 单据编号 varchar 规则：YBM+年月日时分秒+3位随机码, 例如：YBM20160426112240764 -->
		<Field FieldKey="danjubianhao" FieldName="单据编号" FieldType="Varchar"
			Bound="50" DefaultValue="='CG'+o.orderNo()" Comment="单据编号" />
		<Field FieldKey="caigouyuan" FieldName="采购员" FieldType="Varchar"
			Comment="采购员" />
		<Field FieldKey="gysbh" FieldName="供应商编号" FieldType="Varchar"
			Comment="供应商编号" />
		<Field FieldKey="gysmc" FieldName="供应商名称" FieldType="Varchar"
			Comment="供应商名称" />
		<Field FieldKey="jiesuanfangshi" FieldName="结算方式" DefaultValue="1"
			FieldType="Int" Comment="结算方式：1在线支付，2线下转账" />
		<Field FieldKey="sfyfk" FieldName="是否预付款" FieldType="Int"
			Comment="是否预付款" />
		<Field FieldKey="songhuofangshi" FieldName="送货方式" FieldType="Int"
			DefaultValue="1" Comment="送货方式:1公司运输、2客户自提、3委托运输" />
		<!-- <Field FieldKey="qidongshijian" FieldName="启动时间" FieldType="TimeStamp" 
			Comment="启动时间"/> -->
		<!-- <Field FieldKey="daohuoshijian" FieldName="到货时间" FieldType="TimeStamp" 
			Comment="到货时间"/> -->
		<Field FieldKey="yjdhsj" FieldName="预计到货时间" FieldType="Date"
			Comment="预计到货时间" />
		<Field FieldKey="yunshugongju" FieldName="运输工具" FieldType="Int"
			DefaultValue="4" Comment="运输工具" />
		<Field FieldKey="kaipiaoriqi" FieldName="开票日期" FieldType="Date"
			Comment="开票日期" />
		<Field FieldKey="kaipiaoyuan" FieldName="开票员" FieldType="Varchar"
			Comment="开票员" />
		<Field FieldKey="beizhu" FieldName="备注" FieldType="Varchar"
			Comment="备注" />
		<Field FieldKey="shifouzhixing" FieldName="是否执行" FieldType="Int"
			DefaultValue="0" Comment="是否执行" />
		<Field FieldKey="zhsje" FieldName="总含税金额" FieldType="Decimal"
			DefaultValue="0.00" Comment="总含税金额" Bound="14,2" />
		<Field FieldKey="isBackWrite" FieldName="是否回写" FieldType="Int"
			DefaultValue="0" Comment="生成中间表【采购订单】是否生成成功" />
		<Field FieldKey="zhuangtai" FieldName="订单状态" FieldType="Int"
			Comment="0，收货未完成，1，收货完成" />	
	</DataTable>

	<DataTable Key="caigoudingdan_details" BillType="Bill"
		Caption="商品明细" DefaultFilter="BillID" Pagging="100"
		TableName="Table::xyy_wms_bill_caigoudingdan_details" DataSource=""
		Head="false">
		<Field FieldKey="shangpinbianhao" FieldName="商品编号" FieldType="Varchar"
			Comment="商品编号" />
		<Field FieldKey="shangpinmingcheng" FieldName="商品名称" FieldType="Varchar"
			Comment="商品名称" />
		<Field FieldKey="guige" FieldName="规格" FieldType="Varchar"
			Comment="规格" />
		<Field FieldKey="danwei" FieldName="单位" FieldType="Int" Comment="单位" />
		<Field FieldKey="shengchanchangjia" FieldName="生产厂家" FieldType="Varchar"
			Comment="生产厂家" />
		<Field FieldKey="shuliang" FieldName="数量" FieldType="Decimal"
			DefaultValue="0.00" Comment="数量" Bound="14,2" />
		<Field FieldKey="rukushuliang" FieldName="入库数量" FieldType="Decimal"
			DefaultValue="0" Comment="采购入库单回写的数量" Bound="14,2" />
		<Field FieldKey="isPull" FieldName="是否提取" FieldType="Int"
			DefaultValue="0" Comment="入库数量和数量是否相等，是否提取为1" />
		<Field FieldKey="hanshuijia" FieldName="含税价" FieldType="Decimal"
			DefaultValue="0.000" Comment="含税价" Bound="14,3" />
		<Field FieldKey="hanshuijine" FieldName="含税金额" FieldType="Decimal"
			DefaultValue="0.00" Comment="含税金额" Bound="14,2" />
		<Field FieldKey="zuidijinjia" FieldName="最低进价" FieldType="Decimal"
			DefaultValue="0.00" Comment="最低进价" Bound="14,3" />
		<Field FieldKey="zuigaojinjia" FieldName="最高进价" FieldType="Decimal"
			DefaultValue="0.00" Comment="最高进价" Bound="14,3" />
		<Field FieldKey="zuihoujinjia" FieldName="最后进价" FieldType="Decimal"
			DefaultValue="0.00" Comment="最后进价" Bound="14,3" />
		<Field FieldKey="zhjhdw" FieldName="最后进货单位" FieldType="Varchar"
			Comment="最后进货单位" />
		<Field FieldKey="koulv" FieldName="扣率" FieldType="Decimal"
			DefaultValue="100" Comment="扣率" Bound="14,2" />
		<Field FieldKey="shuilv" FieldName="税率" FieldType="Decimal"
			DefaultValue="0.00" Comment="税率" Bound="14,2" />
		<Field FieldKey="pizhunwenhao" FieldName="批准文号" FieldType="Varchar"
			Comment="批准文号" />
		<Field FieldKey="chandi" FieldName="产地" FieldType="Varchar"
			Comment="产地" />
		<Field FieldKey="beizhu" FieldName="备注" FieldType="Varchar"
			Comment="备注" />
		<Field FieldKey="wmsshuliang" FieldName="wms数量" FieldType="Decimal"
			DefaultValue="0.00" Comment="wms数量" Bound="14,2" />	
		<Field FieldKey="zhuangtai" FieldName="订单状态" FieldType="Int"
			Comment="0，收货未完成，1，收货完成" />	
		<!-- <Field FieldKey="lsjgxx" FieldName="历史价格信息" FieldType="Varchar" Comment="历史价格信息"/> -->
	</DataTable>
</DataSet>
