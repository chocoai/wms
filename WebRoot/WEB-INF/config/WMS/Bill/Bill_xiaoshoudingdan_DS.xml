<?xml version="1.0" encoding="utf-8"?>
<DataSet DataSource="" Version="1.0.0">
    <DataTable Key="xiaoshoudingdan" BillType="Bill" Caption="销售订单"
               DefaultFilter="" TableName="Table::xyy_wms_bill_xiaoshoudingdan"
               DataSource="" Head="true">
        <!-- 单据编号 varchar 规则：YBM+年月日时分秒+3位随机码, 例如：YBM20160426112240764 -->
        <Field FieldKey="danjubianhao" FieldName="单据编号" Bound="20"
               FieldType="Varchar" DefaultValue="='XS'+o.orderNo()" Comment="单据编号"/>
        <Field FieldKey="kaipiaoriqi" FieldName="开票日期" FieldType="Date"
               Comment="开票日期"/>
