               Bound="14,2" Comment="订单总额"/>
        <Field FieldKey="youhuijine" FieldName="优惠金额" FieldType="Decimal"
               Bound="14,2" Comment="优惠金额"/>
        <Field FieldKey="hanshuijine" FieldName="含税金额（实付金额）" FieldType="Decimal"
               Bound="14,2" Comment="含税金额（实付金额）"/>
