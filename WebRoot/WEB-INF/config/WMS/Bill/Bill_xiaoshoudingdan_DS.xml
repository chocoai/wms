               DefaultValue="0" Comment="是否出库：1已全部出库，0未全部出库"/>
        <Field FieldKey="shifouchonghong" FieldName="是否冲红" FieldType="Int"
               DefaultValue="0" Comment="是否出库：1已冲红，0未冲红"/>
        <Field FieldKey="shifoutongbu" FieldName="是否同步" FieldType="Int"
               DefaultValue="0" Comment="是否出库：1已同步，0未同步"/>
