<?xml version="1.0" encoding="utf-8"?>
<DataSet DataSource="" Version="1.0.0">
    <DataTable Key="sunyikaipiaodan" Caption="损益开票单" DefaultFilter="" Pagging="10"
               TableName="SQLQuery::select * | from xyy_erp_bill_sunyikaipiaodan where 1 = 1 and status != -1 @danjubianhao  @status order by createTime DESC" DataSource="" Head="false">
        <Parameters> 
            <Parameter Key="danjubianhao" Type="String">
                <Where>
                <![CDATA[ 
					m.if(danjubianhao==null | danjubianhao=="" ," "," and danjubianhao like '%"+danjubianhao+"%'")
	            ]]>
                </Where>
            </Parameter> 
            <Parameter Key="status" Type="Integer" >
                <Where>
                <![CDATA[
                	m.if(status==null ," "," and status="+status+"")
				]]>
                </Where>
            </Parameter>
        </Parameters>
    </DataTable>
</DataSet>